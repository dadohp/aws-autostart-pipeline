    agent any
    stages {
        stage('Build') {
                steps {
                        echo 'hello team'
			
                }
        }
	    stage('Test'){
		    
		steps {
			echo'run test'
        }
	    }
        stage('Deploy') {
 
                steps {
			echo "run deploy's"
                        }
        }
        
    }
}